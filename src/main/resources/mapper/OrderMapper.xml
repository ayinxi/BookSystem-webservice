<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="booksystem.dao.OrderDao">
    <insert id="addOrder">
        INSERT INTO order(id,total,status,return_status,return_reason,check_opinion,address_id,shop_id,user_id,remark,rate,create_time,update_time)
        VALUES(MD5(UUID()),#{total},#{status},#{return_status},#{return_reason},#{check_opinion},#{address_id},#{shop_id},#{user_id},#{remark},#{rate},now(),now());
    </insert>
</mapper>